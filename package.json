{
  "name": "nitrotasks",
  "version": "3.0.0-beta.1",
  "description": "The beautiful way to get things done.",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --colors --mode production --display-optimization-bailout && npm run inject-scripts",
    "watch": "webpack-dev-server --mode development",
    "clean": "rm -rf dist/generated/*",
    "test": "cross-env NODE_ENV=test nyc mocha nitro.sdk.test/index.js && npm run build",
    "test:fast": "cross-env NODE_ENV=test mocha nitro.sdk.test/index.js --require @babel/register",
    "prepublishOnly": "npm run clean && npm run build",
    "flow": "flow",
    "docker-push-master": "docker login -u \"$DOCKER_USERNAME\" -p \"$DOCKER_PASSWORD\" && docker tag dymajo/nitro-client:latest dymajo/nitro-client:stable && docker push dymajo/nitro-client:stable",
    "docker-push-develop": "docker login -u \"$DOCKER_USERNAME\" -p \"$DOCKER_PASSWORD\" && docker push dymajo/nitro-client:latest",
    "prettier": "prettier",
    "inject-scripts": "node build-scripts/inject-scripts.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nitrotasks/nitro.git"
  },
  "author": "",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/nitrotasks/nitro/issues"
  },
  "homepage": "https://nitrotasks.com",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "fake-indexeddb": "^2.0.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.90.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "null-loader": "^0.1.1",
    "nyc": "^13.1.0",
    "offline-plugin": "^5.0.6",
    "prettier": "^1.16.4",
    "resolve-url-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "auth0-js": "^9.9.0",
    "babel-plugin-react-native-web": "^0.9.13",
    "idb-keyval": "^3.1.0",
    "intersection-observer": "^0.5.1",
    "mousetrap": "^1.6.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-beautiful-dnd": "^10.0.3",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.7.0",
    "react-native-web": "^0.9.13",
    "react-router-dom": "^4.3.1",
    "smoothscroll-polyfill": "^0.4.3",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
